    <property>
      <code>.homepage-carousel {
  background-color: #F7F7F7;
  background-image: url("$doc.getAttachmentURL('bg.png')");
  box-shadow: 0 2px 5px #dedede;
  text-shadow: 0 1px 0 #fff;
}

@media (min-width: 768px) {
  .homepage-carousel {
    margin: 0 -100%;
    padding: 0 100%;
    position: absolute;
    top: 0;
    left: 0;
  }

  .homepage-content {
    padding-top: 22em;
  }

  #contentcontainer {
    position: relative;
  }

  #body #contentcontainer #contentcolumn {
    position: inherit;
  }
}

@media (min-width: 480px) {
  .homepage-carousel .section {
    background-color: rgba(255, 255, 255, 0.5);
  }

  .main-wiki .homepage-carousel {
    background: 
    url("/xwiki/bin/download/Carousel/Section0/abstract.png") 180% 0 no-repeat,
    url("$doc.getAttachmentURL('bg.png')") 0 0 repeat;
  }
}

@media (min-width: 768px) {
  .homepage-carousel .section {
    background-color: transparent;
  }

  .main-wiki .homepage-carousel {
    background: 
    url("/xwiki/bin/download/Carousel/Section0/abstract.png") 80% 0 no-repeat,
    url("/xwiki/bin/download/Carousel/Section0/abstract.png") 20% 0 no-repeat,
    url("$doc.getAttachmentURL('bg.png')") 0 0 repeat;
  }

  .extension-wiki .homepage-carousel {
    background: 
    url("/xwiki/bin/download/Carousel/Section2/extended.png") 20% 50% no-repeat,
    url("$doc.getAttachmentURL('bg.png')") 0 0 repeat;
  }
}</code>
