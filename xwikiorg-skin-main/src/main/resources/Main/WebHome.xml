    <property>
      <code>/* --------------------------------------------------------------
  * @group Layout
-------------------------------------------------------------- */

.homepage-content .latestpostsContainer, .homepage-content .poweredByContainer, .homepage-content .heartbeatContainer {
  float: left;
  padding-left: 0.75%;
  padding-right: 0.75%;
}

.homepage-content .poweredByContainer {
  padding-left: 0;
  z-index: 2;
}

.homepage-content .heartbeatContainer {
  padding-right: 0;
}

@media (max-width:1024px) {
  .homepage-content .latestpostsContainer {
    width: 48.9%;
  }

  .homepage-content .poweredByContainer, .homepage-content .heartbeatContainer {
    width: 24%;
  }
}

@media (min-width:1024px) {
  .homepage-content .latestpostsContainer {
    padding: 0 250px 0 23.5%;
  }

  .homepage-content .poweredByContainer, .homepage-content .heartbeatContainer {
    position: absolute;
  }

  .homepage-content .poweredByContainer{
    margin-right: -24%;
    width: 21%;
  }

  .homepage-content .heartbeatContainer { 
    margin-left: -250px;
    right: 0;
    width: 250px;
  }
}

/* --------------------------------------------------------------
  * @group Latest Blog Posts
-------------------------------------------------------------- */
@media (max-width:1360px) {
  .latestpostsContainer {
    height: 26em;
    margin-bottom: 1em;
  }
}

@media (min-width:1360px) {
  .homepage-content .latestposts .entry-summary {
    max-height: 5.5em;
  }

  .homepage-content .latestposts .entry-summary * {
    margin-top: 0;
    margin-bottom: 0;
  }
  .latestpostsContainer {
    height: 29em;
  }
}

@media \0screen { /* IE8 hack since it doesn't have support for media queries */
  .latestpostsContainer {
    height: 27em;
  }
}

@media (min-width:1800px) {
  .latestpostsContainer {
    height: 28.5em;
  }
}

/* --------------------------------------------------------------
  * @group Powered By
-------------------------------------------------------------- */
@media (min-width:1360px) {
  .homepage-content .poweredBy .description {
    max-height: 5.5em;
  }
}

/* --------------------------------------------------------------
  * @group Carousel
-------------------------------------------------------------- */
@media (max-width:1024px) {
  .slide .slide-picture .screenshots {
    background-position: 0% 50%;
  }
